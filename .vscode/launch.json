                    "kind": "bin"
                },
                "env": {
                    "RUSTFLAGS": "--cfg async_executor_impl=\"tokio\" --cfg async_channel_impl=\"tokio\" -C linker=clang -C link-arg=-fuse-ld=/home/pine/.local/bin/mold"
                }
