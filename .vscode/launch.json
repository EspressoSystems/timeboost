    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'tests'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=tests"
                ],
                "filter": {
                    "name": "tests",
                    "kind": "lib"
                },
                "env": {
                    "RUSTFLAGS": "--cfg async_executor_impl=\"tokio\" --cfg async_channel_impl=\"tokio\" -C linker=clang -C link-arg=-fuse-ld=/home/pine/.local/bin/mold"
                }
            },
            "args": ["test_simple_network::test_simple_network_genesis_message"],
            "cwd": "${workspaceFolder}",
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'sailfish'",
